public class BigRedApple extends Apple {

	private static final long serialVersionUID = 1L;

	/**
	 * overriding,重写方法<br>
	 * 异常无法扩大
	 */
	@Override
	public void bloomFlower() {

	}
	
	/**
	 * overloading,重载方法<br>
	 * @param color
	 */
	public void bloomFlower(String color)throws Exception {
		
	}
}
