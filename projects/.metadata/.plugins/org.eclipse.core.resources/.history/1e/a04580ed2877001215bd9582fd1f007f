package com.hundsun.fund.java1;

import java.io.Serializable;

/**
 * 类名,必须大写
 * @author laiqinyi
 *
 */
public interface Fruit extends Serializable {
	
	public static final String TYPE_NAME = "水果";
	//所有接口声明的成员变量都是public static final
	String CATEGORY_NAME = "植物";
	
	
	public abstract int grow(int rate);
	void bloom();
	
}
