package com.hundsun.fund.java7.inner_class;

public class AnonymousInner {

	class Popcorn {
		public void pop() {
			System.out.println("popcorn");
		}
	}

	

	public static void main(String[] args) {
		Popcorn p = new Popcorn() {
			public void pop() {
				System.out.println("anonymous popcorn");
			}
		};
	}

}
