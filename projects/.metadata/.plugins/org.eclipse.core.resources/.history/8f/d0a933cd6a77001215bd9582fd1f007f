package com.hundsun.fund.java4.flowcontrol_and_exceptions;

public class MainOfExceptions {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int i = 0;
		try {
			i = 100/0;
		} catch (ArithmeticException ex1) {
			ex1.printStackTrace();
		}catch(RuntimeException ex2){
			ex2.printStackTrace();
		}
		
	}

}
